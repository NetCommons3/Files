{
    "name": "netcommons/files",
    "description": "Files for NetCommons Plugin",
    "homepage": "http://www.netcommons.org/",
    "extra": {
        "installer-paths": {
            "app/Plugin/{$name}": ["type:cakephp-plugin"]
        }
    },
    "require": {
        "cakedc/migrations":            "~2.2",
        "josegonzalez/cakephp-upload":  "2.x@dev",
        "netcommons/authorization-keys": "dev-master",
        "netcommons/blocks":            "dev-master",
        "netcommons/net-commons":       "dev-master"
        "netcommons/site-manager":       "dev-master"
    },
    "license": "NetCommons License",
    "authors": [
        {
            "name": "NetCommons Community",
            "homepage": "https://github.com/NetCommons3/Files/graphs/contributors"
        }
    ],
    "type":        "cakephp-plugin",
    "keywords":    ["cakephp", "files"],
    "config": {
        "vendor-dir": "vendors"
    }
}

